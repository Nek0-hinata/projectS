    "@heroicons/react": "2.1.5",
    "@prisma/client": "5.20.0",
    "@vercel/analytics": "1.3.1",
    "@vercel/postgres": "0.10.0",
    "@vercel/speed-insights": "1.0.12",
    "ahooks": "3.8.1",
    "antd": "5.21.1",
    "bcrypt": "5.1.1",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "lodash": "4.17.21",
    "next": "14.2.13",
    "next-auth": "5.0.0-beta.22",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.23.8"
